    "shipped": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "boolean"
    },
    "return": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "boolean"
    },
    "delivered": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "boolean"
    }
  }
