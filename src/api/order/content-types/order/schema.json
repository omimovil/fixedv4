      "type": "relation",
      "relation": "manyToMany",
      "target": "api::product.product",
      "mappedBy": "orders"
    },
    "admin_users": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "admin::user"
    },
    "total_price": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
